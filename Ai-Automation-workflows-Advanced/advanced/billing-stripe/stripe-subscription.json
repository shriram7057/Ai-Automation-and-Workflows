{
  "name": "Stripe Subscription",
  "nodes": [
    {
      "name": "Stripe Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        100,
        300
      ],
      "parameters": {
        "path": "stripe",
        "httpMethod": "POST"
      }
    },
    {
      "name": "Stripe Update",
      "type": "n8n-nodes-base.stripe",
      "typeVersion": 1,
      "position": [
        350,
        300
      ],
      "parameters": {
        "resource": "customer",
        "operation": "update"
      }
    },
    {
      "name": "Slack Alert",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [
        600,
        300
      ],
      "parameters": {
        "channel": "#billing",
        "text": "Subscription updated"
      }
    }
  ],
  "connections": {
    "Stripe Webhook": {
      "main": [
        [
          {
            "node": "Stripe Update",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Stripe Update": {
      "main": [
        [
          {
            "node": "Slack Alert",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
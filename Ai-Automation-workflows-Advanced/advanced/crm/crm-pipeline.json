{
  "name": "CRM Pipeline",
  "nodes": [
    {
      "name": "Lead Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [
        100,
        300
      ],
      "parameters": {
        "path": "lead",
        "httpMethod": "POST"
      }
    },
    {
      "name": "HubSpot Lead",
      "type": "n8n-nodes-base.hubspot",
      "typeVersion": 1,
      "position": [
        350,
        300
      ],
      "parameters": {
        "resource": "contact",
        "operation": "create"
      }
    },
    {
      "name": "AI Scoring",
      "type": "n8n-nodes-base.openAi",
      "typeVersion": 1,
      "position": [
        600,
        300
      ],
      "parameters": {
        "model": "gpt-4"
      }
    }
  ],
  "connections": {
    "Lead Webhook": {
      "main": [
        [
          {
            "node": "HubSpot Lead",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "HubSpot Lead": {
      "main": [
        [
          {
            "node": "AI Scoring",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}